{
  "framework": "vite",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://leadclinic.net"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://leadclinic.net https://partnerb2b.com.br"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Accept"
        }
      ]
    },
    {
      "source": "/api/(.*) ",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json; charset=utf-8"
        }
      ]
    }
  ],
  "functions": {
    "api/chat.js": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
}